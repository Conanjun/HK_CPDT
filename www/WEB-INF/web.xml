<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.4">
  <context-param>
    <param-name>webAppRootKey</param-name>
    <param-value>wizBank</param-value>
  </context-param>
  <context-param>
    <param-name>log4jConfigLocation</param-name>
    <param-value>/WEB-INF/config/log4j.properties</param-value>
  </context-param>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>/WEB-INF/applicationContext.xml</param-value>
  </context-param>
  <context-param>
    <param-name>weblogic.httpd.inputCharset./*</param-name>
    <param-value>UTF-8</param-value>
  </context-param>
  <filter>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
    <init-param>
      <param-name>forceEncoding</param-name>
      <param-value>true</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
   <!-- 默认不打开文件访问安全过滤-->
  <filter>
    <filter-name>SuperSessionFilter</filter-name>
    <filter-class>com.cw.wizbank.util.SuperSessionFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SuperSessionFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>




  
    <filter>
    <filter-name>SiteMeshFilter</filter-name>
    <filter-class>com.opensymphony.sitemesh.webapp.SiteMeshFilter</filter-class>
  </filter>
  
  <filter-mapping>
    <filter-name>SiteMeshFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  
    <filter>
  	<filter-name>xmlFilter</filter-name>
  	<filter-class>com.cwn.wizbank.web.XMLFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>xmlFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  
  
  <servlet>
    <servlet-name>qdbAction</servlet-name>
    <servlet-class>com.cw.wizbank.qdb.qdbAction</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>Dispatcher</servlet-name>
    <servlet-class>com.cw.wizbank.Dispatcher</servlet-class>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>aeAction</servlet-name>
    <servlet-class>com.cw.wizbank.ae.aeAction</servlet-class>
    <load-on-startup>4</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>CMI</servlet-name>
    <servlet-class>com.cw.wizbank.aicc.CMI</servlet-class>
    <load-on-startup>6</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>SCO2004CMI</servlet-name>
    <servlet-class>com.cw.wizbank.aicc.SCO2004CMI</servlet-class>
    <load-on-startup>9</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>Cert</servlet-name>
    <servlet-class>com.cw.wizbank.cert.CertServlet</servlet-class>
    <load-on-startup>7</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>initLabel</servlet-name>
    <servlet-class>com.cwn.wizbank.web.InitServlet</servlet-class>
    <load-on-startup>10</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>DispatcherServlet</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>
      <param-name>contextConfigLocation</param-name>
      <param-value>/WEB-INF/mvc-servlet.xml</param-value>
    </init-param>
    <init-param>
      <param-name>detectAllHandlerExceptionResolvers</param-name>
      <param-value>false</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>DispatcherServlet</servlet-name>
    <url-pattern>/app/*</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
  </listener>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.cwn.wizbank.web.WzbSitemeshListener</listener-class>
  </listener>
  <servlet-mapping>
    <servlet-name>qdbAction</servlet-name>
    <url-pattern>/servlet/qdbAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Dispatcher</servlet-name>
    <url-pattern>/servlet/Dispatcher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>aeAction</servlet-name>
    <url-pattern>/servlet/aeAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CMI</servlet-name>
    <url-pattern>/servlet/CMI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CMI</servlet-name>
    <url-pattern>/servlet/cmi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SCO2004CMI</servlet-name>
    <url-pattern>/servlet/SCO2004CMI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Cert</servlet-name>
    <url-pattern>/servlet/cert</url-pattern>
  </servlet-mapping>
  <mime-mapping>
    <extension>doc</extension>
    <mime-type>application/msword</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>xls</extension>
    <mime-type>application/vnd.ms-excel</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>ppt</extension>
    <mime-type>application/vnd.ms-powerpoint</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>asf</extension>
    <mime-type>application/vnd.ms-asf</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>asx</extension>
    <mime-type>application/vnd.ms-asf</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>exe</extension>
    <mime-type>application/octet-stream</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>pdf</extension>
    <mime-type>application/pdf</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>dcr</extension>
    <mime-type>application/x-director</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>zip</extension>
    <mime-type>application/zip</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>rar</extension>
    <mime-type>application/rar</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>swf</extension>
    <mime-type>application/x-shockwave-flash</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>midi</extension>
    <mime-type>audio/midi</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>rm</extension>
    <mime-type>audio/x-pn-realaudio</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>ram</extension>
    <mime-type>audio/x-pn-realaudio</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>ra</extension>
    <mime-type>audio/x-realaudio</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>wav</extension>
    <mime-type>audio/x-wav</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>wma</extension>
    <mime-type>audio/x-wma</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>avi</extension>
    <mime-type>video/x-msvideo</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>mp4</extension>
    <mime-type>video/mp4</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>mov</extension>
    <mime-type>video/quicktime</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>mpg</extension>
    <mime-type>video/mpeg</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>mpeg</extension>
    <mime-type>video/mpeg</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>mpe</extension>
    <mime-type>video/mpeg</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>cp</extension>
    <mime-type>application/cp</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>xlsx</extension>
    <mime-type>application/vnd.ms-excel</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>docx</extension>
    <mime-type>application/msword</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>pptx</extension>
    <mime-type>application/vnd.ms-powerpoint</mime-type>
  </mime-mapping>
  <resource-ref>
    <res-ref-name>jdbc/wizbankds</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <jsp-config>
    <taglib>
      <taglib-uri>/wizbank/label</taglib-uri>
      <taglib-location>/WEB-INF/tld/label.tld</taglib-location>
    </taglib>
  </jsp-config>
  <filter>
    <display-name>XMLFilter</display-name>
    <filter-name>XMLFilter</filter-name>
    <filter-class>com.cwn.wizbank.web.XMLFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>XMLFilter</filter-name>
    <url-pattern>/XMLFilter</url-pattern>
  </filter-mapping>
</web-app>